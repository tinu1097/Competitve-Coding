   "source": [
    "n = int(input())\n",
    "m = list(map(int,input().split(\" \")))\n",
    "x = 0\n",
    "y = 0\n",
    "z = 0\n",
    "if len(m)==n:\n",
    "  for i in m:\n",
    "    if i>0:\n",
    "      x = x+1\n",
    "    elif i<0:\n",
    "      y = y+1\n",
    "    else:\n",
    "      z = z+1\n",
    "print(\"{:.6f}\".format(x/len(m)))\n",
    "print(\"{:.6f}\".format(y/len(m)))\n",
    "print(\"{:.6f}\".format(z/len(m))) "
   ]
  }
