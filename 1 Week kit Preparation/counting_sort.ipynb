    "n = int(input())\n",
    "x = sorted(list(map(int,input().split(\" \"))))\n",
    "if len(x)==n:\n",
    "    c = dict()\n",
    "    for i in x:\n",
    "        if i in c:\n",
    "            c[i]+=1\n",
    "        else:\n",
    "            c[i]=0\n",
    "    for j in c.values():\n",
    "        print(j,end=\" \")\n",
    "    "
